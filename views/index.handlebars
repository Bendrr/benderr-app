<div class="row">
    <div class="col-md-7">
        <form class="form-inline">
            <h2>Don't Drink Alone.
                    <br>Sign In to find your drinking buddy!</h2>
            <div class="form-group">
                <label for="exampleInputEmail1"></label>
                <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Email">
            </div>
            <div class="form-group">
                <label for="exampleInputPassword1"></label>
                <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
            </div>
            <button type="submit" id="login" class="btn btn-default">Submit</button>
    </div>
    </form>
    <div class="col-md-5">
        <form class="form-inline">
            <h2>New Here? Sign Up</h2>
            <div class="form-group">
                <label for="firstNameInput"></label>
                <input type="name" class="form-control" id="firstNameInput" placeholder="First Name">
            </div>
            <div class="form-group">
                <label for="lastNameInput"></label>
                <input type="name" class="form-control" id="lastNameInput" placeholder="Last Name">
            </div>
        </form>
        <br>
        <form class="form-inline">
            <div class="form-group">
                <label for="InputEmail"></label>
                <input type="email" class="form-control" id="InputEmail" placeholder="Email">
            </div>
            <div class="form-group">
                <label for="InputPassword"></label>
                <input type="password" class="form-control" id="InputPassword" placeholder="Password">
            </div>
        </form>
        <br>
        <form class="form-inline">
            <label for="age"></label>
            <input type="age" class="form-control" id="age" placeholder="Age">
            <label for="phoneNumber "></label>
            <input type="tel " class="form-control " id="phoneNumber" placeholder="Telephone ">
        </form>
        <br>
        <form class="form-inline">
            <input type="radio" name="gender" value="male" checked> Male
            <input type="radio" name="gender" value="female"> Female
        </form>
        <br>
        <form class="form-inline">
            <label for="tagline "></label>
            <input type="tag " class="form-control " id="tagline" placeholder="Type Your Tagline">
        </form>
        <br>
        <div class="form-group">
                <label for="profile_pic"></label>
                <input type="pic" class="form-control" id="profilePic" placeholder="Photo URL">
            </div>
            <button type="submit" id="createUser" class="btn btn-default">Submit</button>
        </form>
    </div>
</div>
